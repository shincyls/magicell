<%= form_for @user, id: 'new-user-form', remote: true  do |f| %>

    <h5 class="text-special text-success">User Webrole</h5>
    <div class="row text-special text-dark mt-2 mb-3">
      <div class="col-lg-3 col-md-6">
        <label>Webrole</label>
        <%= f.select(:webrole_id, Webrole.all.collect {|l| [ l.role, l.id ] }, {include_blank: "Please Select"}, class: "custom-select custom-select-sm") %>
      </div>
    </div>

<% end %>



