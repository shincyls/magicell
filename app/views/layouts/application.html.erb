<!DOCTYPE html>
<html>
  <head>
    <title>Prins Solution</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= favicon_link_tag 'brand/favicon.png' %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
 
  </head>

  <body>
    <div id="flash-message"><%= render partial: 'statics/message' %></div>
    <%= render partial: 'statics/navbar' %>
    <div class="h-100">
      <%= yield %>
    </div>
    <%= render partial: 'statics/modal' %>
    <%= render partial: 'statics/footer' %>
  </body>

</html>
