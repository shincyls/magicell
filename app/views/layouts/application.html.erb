<!DOCTYPE html>
<html>
  <head>
    <title>Magicell Sdb Bhd</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= favicon_link_tag 'brand/favicon.png' %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    
    <link href="https://fonts.googleapis.com/css?family=Roboto|Lato|Montserrat|Chivo|Righteous|Pacifico" rel="stylesheet">
    <link href="https://cdn.datatables.net/1.10.19/css/jquery.dataTables.min.css" rel="stylesheet">
    <script src="https://cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js"></script>

  </head>

  <body>
    <div id="flash-message"><%= render partial: 'statics/message' %></div>
    <%= render partial: 'statics/navbar' %>

    <div class="container">
      <%= yield %>
    </div>
    
    <%= render partial: 'statics/modal' %>
    <%= render partial: 'statics/footer' %>
    </div>
  </body>


</html>

