        <% e = @project %>
        <td><%=e.name%></td>
        <td><%=e.manager.full_name if e.manager %></td>
        <td><%=e.manager_alt.full_name if e.manager_alt %></td>
        <td><%=e.description%></td>
        <td><%=e.vendor%></td>
        <td><%=e.operator%></td>
        <td><%=e.project_status%></td>
        <% if current_user.webrole.wr_hr %>
        <td>
          <span class="bg-success rounded p-1 m-1"><%= link_to 'Edit', edit_project_path(e), class: "small text-white m-1", remote: true %></span>
        </td>
        <% end %>